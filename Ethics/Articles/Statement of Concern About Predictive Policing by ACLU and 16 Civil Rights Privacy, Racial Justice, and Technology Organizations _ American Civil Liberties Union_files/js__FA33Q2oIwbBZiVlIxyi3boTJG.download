(function ($, Drupal, window, document, undefined) {
  
  Drupal.behaviors.acluSocialClickToCallAction = {
    attach: function (context, settings) {
      // Needs to be on submit for HTML5 form validation.
      // Required fields in the submitting form must follow html5 validation specifications
      $('#aclu-call-signup-form', context).data('submitting', "false");
      $('#aclu-call-signup-form', context).once('aclu-social-click-form-submit-replace').submit(function(e) {
        // prevent double submit
        $('button[type=submit]', this).attr('disabled', 'disabled');
        if ($(this).data('submitting') === "false") {
          $(this).data('submitting', "true");
          // The aclu_social.module contains the /click-to-call-api springboard post to api code
          // NOTE: the springboard form has an implicit contact me by email optin if email is submitted. And Email is a required field.
          // the springboard form also has an implict contact me by phone optin if a phone number is submitted. 
          // Phone and email should be required on the click to call form. If the required fields are not filled in. Do not submit the form.
          var queryStrings = Drupal.behaviors.acluSocialGetQueryParams(document.location.search);
          var posting = $.post('/click-to-call-api',
          {
            phone: $('#phone').val(),
            profile_first_name: $('#first_name').val(),
            mail: $('#email').val(),
            profile_zip: $('#zip').val(),
            ms_aff: queryStrings.hasOwnProperty('ms_aff') ? queryStrings['ms_aff'] : '',
            ms_chan: queryStrings.hasOwnProperty('ms_chan') ? queryStrings['ms_chan'] : '',
            ms: queryStrings.hasOwnProperty('ms') ? queryStrings['ms'] : '',
            initms_aff: queryStrings.hasOwnProperty('initms_aff') ? queryStrings['initms_aff'] : '',
            initms_chan: queryStrings.hasOwnProperty('initms_chan') ? queryStrings['initms_chan'] : '',
            initms: queryStrings.hasOwnProperty('initms') ? queryStrings['initms'] : '',
            utm_medium: queryStrings.hasOwnProperty('utm_medium') ? queryStrings['utm_medium'] : '',
            utm_source: queryStrings.hasOwnProperty('utm_source') ? queryStrings['utm_source'] : '',
            utm_content: queryStrings.hasOwnProperty('utm_content') ? queryStrings['utm_content'] : '',
            utm_campaign: queryStrings.hasOwnProperty('utm_campaign') ? queryStrings['utm_campaign'] : '',
            campaign_id: $('#campaign_id').val(),
            node_id: Drupal.settings.aclu_social.node_id,
            node_title: Drupal.settings.aclu_social.node_title,
            staging: queryStrings.hasOwnProperty('staging') ? queryStrings['staging'] : '',
          }
          )
          posting.always(function() {
            $('#aclu-call-signup-form').submit();
          });
        }
      }); 
    }
  }
})(jQuery, Drupal, this, this.document);

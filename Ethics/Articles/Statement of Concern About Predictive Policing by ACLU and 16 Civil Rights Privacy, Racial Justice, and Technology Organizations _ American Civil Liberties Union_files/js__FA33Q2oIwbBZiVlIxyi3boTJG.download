  // Blog Post Featured Action: Move inside description if long enough.
  Drupal.behaviors.blogFeaturedAction = {
    attach: function (context, settings) {
      var featuredAction = $('.node-type-blog .pane-featured-action');
      var numParagraphs = Math.round(($('.node-type-blog .pane-aclu-components-description p').length) / 2);
      var numParagraphsNext = numParagraphs + 1;
      var totalParagraphs = $('.node-type-blog .pane-aclu-components-description p').length;
      var numPars, noImage;

      if ($('.node-type-blog .pane-aclu-components-description').text().length < 3000) {
        $('body').addClass('blog-without-long-description');
      }
      else {
        $('body').addClass('blog-with-long-description');
      }

      Response.action(function() {
        // If blog post is short or long, add featured Action to the middle of paragraph count
        if (Response.band(Breakpoints.SMALL, Breakpoints.LARGE)) {
          $('.node-type-blog.blog-with-long-description', context).once('blog-with-long-description', function() {

            if (($('.node-type-blog.blog-with-long-description p:eq(' + numParagraphs + ')').find('img').length > 0) || ($('.node-type-blog.blog-with-long-description p:eq(' + numParagraphsNext + ')').find('img').length > 0)) {
              // Logic for determining Action blocks from not appearing within a <p> with an image in it
              for (i = numParagraphs; i <= totalParagraphs; i++) {
                var curPar = 'false';
                var nextPar = 'false';
                var numLoop = 'true';
                var postPar = i + 1;
                if ($('.node-type-blog.blog-with-long-description p:eq(' + i + ')').find('img').length > 0) {
                  curPar = true;
                }
                if ($('.node-type-blog.blog-with-long-description p:eq(' + postPar + ')').find('img').length > 0) {
                  nextPar = true;
                }
                // If image is found within current paragraph or following paragraph throughout the entire content,
                // let's add the Action block to the very end of the article
                if ((curPar == 'false') && (nextPar == 'false')) {
                  numPars = i;
                  return false;
                }
              }
            }
            else {
              noImage = 'true';
            }
          });
          // Action block needs to be added towards the bottom of the last paragraph
          if (totalParagraphs == numPars) {
            numPars = numPars - 1;
          }
          $(featuredAction).prependTo($('.node-type-blog.blog-without-long-description .pane-aclu-components-description p:eq(' + numParagraphs + ')'));
          // If there aren't any images found within <p>, place Action promo within middle of post
          if (noImage == 'true') {
            $(featuredAction).prependTo($('.node-type-blog.blog-with-long-description .pane-aclu-components-description p:eq(' + numParagraphs + ')'));
            return false;
          }
          else {
            $(featuredAction).prependTo($('.node-type-blog.blog-with-long-description .pane-aclu-components-description p:eq(' + numPars + ')'));
          }
        }
        if (Response.band(Breakpoints.LARGE)) {
          // If blog post is short, add featured Action to the right column
          $(featuredAction).insertAfter($('.node-type-blog.blog-without-long-description .sidebar-right .pane-aclu-blog-news-signup'));
          // If blog post is long, add featured Action to the middle of paragraph count
          $(featuredAction).prependTo($('.node-type-blog.blog-with-long-description .pane-aclu-components-description p:eq(' + numParagraphs + ')'));
        }
      });
    }
  };


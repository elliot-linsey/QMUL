  Drupal.behaviors.getUpdatesForm = {
    attach: function(context, settings) {
      var $getUpdatesButton = $('.menu-item-get-updates', context).once(),
          $getUpdatesForm = $('#block-aclu-social-get-updates', context);
      if (!$getUpdatesButton.length) {
        return;
      }
      var closeUpdatesForm = function() {
        $getUpdatesForm.hide().removeClass('open');
        $getUpdatesButton.removeClass('open');
      },
      openUpdatesForm = function() {
        if (formCanBeShown()) {
          $getUpdatesButton.addClass('open');
          $getUpdatesForm.slideDown('fast').addClass('open');
          $getUpdatesForm.find('.form-item-signup-email .form-text').focus();
          $(window).trigger('getUpdatesForm:open');
        }
      },
      toggleUpdatesForm = function() {
        if ($getUpdatesForm.hasClass('open')) {
          closeUpdatesForm();
        }
        else {
          openUpdatesForm();
        }
      },
      formCanBeShown = function(){
        if ($('body').hasClass('layout-medium') || $('#header').hasClass('sticky-minimized')) {
          return true;
        }
        else {
          return false;
        }
      };

      $getUpdatesButton
      .once(function() {
        $(window).on('headerSearch:open', function() {
          closeUpdatesForm();
        });
      })
      // When you click the Get Updates button...
      .on('click', function(e) {
        // Keep link from functioning as a link
        e.preventDefault();
        toggleUpdatesForm();
      });

      // Clicking anything else will close the form.
      $('body').mousedown(function(e) {
        // Exclude the button and form.
        if (
          $(e.target).closest($getUpdatesForm).length ||
          $(e.target).is($getUpdatesButton)
        ) {
          return;
        }
        closeUpdatesForm();
      });

      // When tabbing out of form going forwards...
      $getUpdatesForm.find('.form-submit').keydown(function(e) {
        // 9 is tab key.
        if (formCanBeShown() && e.which === 9 && e.shiftKey !== true) {
          closeUpdatesForm();
          // And put focus on the Donate button
          $('#menu-item-donate', context).focus();
        }
      });
      // When tabbing out of form going backwards...
      $getUpdatesForm.find('.form-item-signup-email input').keydown(function(e) {
        // 9 is tab key.
        if (formCanBeShown() && e.which === 9 && e.shiftKey === true) {
          closeUpdatesForm();
          // And put focus on the Updates button
          $getUpdatesButton.focus();
        }
      });

    }
  };


  // Angled Pull Quote component height
    // ////////////////////////////////////////////////////////////////

  // String.repeat is only available in ECMA6, so most browsers don't have it.
  if (!String.prototype.repeat) {
    String.prototype.repeat = function(num) {
      return new Array(num + 1).join(this);
    };
  }

  Drupal.behaviors.pullQuoteAngled = {
    attach: function(context, settings) {

      $('.pullquote__angled', context).once().each(function() {

        var $pullQuote = $(this);

        // Add the extra spans required (because CSS Shapes isn't production ready).
        $pullQuote
          .wrapInner('<span class="pullquote__text"></span>')
          .prepend('<span class="push"></span>'.repeat(20));

        // Call height function upon page load, and again whenever page is resized.
        setPullquoteHeight($pullQuote);
        // @todo Wrap the anonymous function in $.debounce(300, );
        $(window).resize(function() {
          setPullquoteHeight($pullQuote);
        });

        // Set the height of the pullquote to be the height of its text.
        function setPullquoteHeight($pullQuote) {
          var $pullquoteText = $('.pullquote__text', $pullQuote);
          var height = $pullquoteText.height();
          $pullQuote.css('height', height);
        }

        var $push = $('.push', $pullQuote);

        // Set the widths of the "push" divs at different paces for the breakpoints.
        Response.action(function() {
          // Small layouts
          if (Response.band(0, 419)) {
            $push.each(function(i) {
              $(this).css('width', ((++i * 7) + 12) + 'px');
            });
          }

          // Small-Medium layouts
          if (Response.band(420, 767)) {
            $push.each(function(i) {
              $(this).css('width', ((++i * 8) + 16) + 'px');
            });
          }

          // Medium layouts
          if (Response.band(768)) {
            $push.each(function(i) {
              $(this).css('width', ((++i * 13) + 21) + 'px');
            });
          }
        });

      });

      // Give Colorbox buttons text
      Drupal.behaviors.colorBoxButtons = {
        attach: function(context, settings) {
          $('#cboxPrevious').text('Previous');
          $('#cboxNext').text('Next');
          $('#cboxSlideshow').text('Slideshow');
        }
      };

    }
  };


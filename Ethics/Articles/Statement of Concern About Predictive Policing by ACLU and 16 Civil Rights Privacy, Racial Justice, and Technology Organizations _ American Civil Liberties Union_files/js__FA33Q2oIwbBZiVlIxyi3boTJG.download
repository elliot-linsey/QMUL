/**
 * @file
 * Contains custom functionality related to positioning and moving elements
 * based on the browser size. All mobile/responsive JS should go here.
 */
(function ($, Drupal, window, document, R) {
  var acluResponsive = {};

  Drupal.AcluResponsive = Drupal.AcluResponsive || {};

  // A list of breakpoints we are using. This variable is not actually used
  // anywhere important. Rather, it can be used as constants so you don't have
  // to hard-code your breakpoints everywhere. For example, you can use
  // "Breakpoints.TABLET" instead of "768".
  Drupal.AcluResponsive.Breakpoints = Breakpoints = {
    SMALL: 320,
    EMBED_MEDIUM: 600,
    MEDIUM: 768,
    LARGE: 960,
    WIDE: 1200  };

    // TODO: breakpoints need to be R.band(0, 767) and R.band(768) so there is no overlapping of functions at 768px, for example. Currently both functions are called at that specific breakpoint (768, 960,1200).

  Response.create({
    prop: "width"  // "width" "device-width" "height" "device-height" or "device-pixel-ratio"
    , prefix: "min-width- r src"  // the prefix(es) for your data attributes (aliases are optional)
    , breakpoints: [0, 769, 961, 1201] // min breakpoints (defaults for width/device-width)
  });

  // Add layout breakpoint bodyclasses
  Drupal.behaviors.breakpointBodyClass = {
    attach: function (context, settings) {

      Response.action(function() {

        $('body', context).each(function() {
          if (Response.band(Breakpoints.SMALL, Breakpoints.MEDIUM)) {
            $(this).addClass('layout-small').removeClass('layout-medium layout-large layout-wide');
          }
          if (Response.band(Breakpoints.MEDIUM, Breakpoints.LARGE)) {
            $(this).addClass('layout-medium').removeClass('layout-small layout-large layout-wide');
          }
          if (Response.band(Breakpoints.LARGE, Breakpoints.WIDE)) {
            $(this).addClass('layout-large').removeClass('layout-small layout-medium layout-wide');
          }
          if (Response.band(Breakpoints.WIDE)) {
            $(this).addClass('layout-wide').removeClass('layout-small layout-medium layout-large');
          }
        });

      });
    }
  }

  // Sticky Subnav
  Drupal.behaviors.stickyNav = {
    attach: function (context, settings) {
      var $menus = $('.sticky-nav', context),
        isMobile = R.band(0, Breakpoints.MEDIUM);

      $menus.each(function(index, element) {
        var $menu = $(element),
            isSticky = !!$menu.parent('.sticky-wrapper').length;

        if (isMobile && isSticky) {
          $menu.unstick();
        } else if (!isSticky) {
          $menu.sticky();
        }
      });
    }
  };

  //////
  // Misc. Behaviors
  //////

  // Scrollbar Swiper
  Drupal.behaviors.scrollbarSwiper = {
    attach: function (context, settings) {

      $(".scrollbar-swiper", context).each(function(index, element) {
        var totalWidth = 0;
        var mouseEnabled = true;
        if (Response.band(Breakpoints.LARGE) || Response.band(Breakpoints.WIDE)) {
          mouseEnabled = false;
        }
        $(this).find('article').each(function(){
          totalWidth += $(this).outerWidth(true);
        });
        $(this).find('.window').css("width", totalWidth);
        $(this).mCustomScrollbar({
          axis: "x", // horizontal scrollbar
          scrollbarPosition: "outside",
          autoDraggerLength: false,
          mouseWheel:{ enable: mouseEnabled }
        });
        // Small layouts
        Response.action(function() {
          if (Response.band(Breakpoints.SMALL)) {
            var totalWidth = 0;
            $(this).find('article').each(function(){
              totalWidth += $(this).outerWidth(true);
            });
            $(this).find('.window').css("width", totalWidth);
            $(this).mCustomScrollbar("update");
          }
        });
        // Medium layouts
        Response.action(function() {
          if (Response.band(Breakpoints.MEDIUM)) {
            var totalWidth = 0;
            $(this).find('article').each(function(){
              totalWidth += $(this).outerWidth(true);
            });
            $(this).find('.window').css("width", totalWidth);
            $(this).mCustomScrollbar("update");
          }
        });
        // Large layouts
        Response.action(function() {
          if (Response.band(Breakpoints.LARGE)) {
            var totalWidth = 0;
            $(this).find('article').each(function(){
              totalWidth += $(this).outerWidth(true);
            });
            $(this).find('.window').css("width", totalWidth);
            $(this).mCustomScrollbar("update");
          }
        });
        // Wide layouts
        Response.action(function() {
          if (Response.band(Breakpoints.WIDE)) {
            var totalWidth = 0;
            $(this).find('article').each(function(){
              totalWidth += $(this).outerWidth(true);
            });
            $(this).find('.window').css("width", totalWidth);
            $(this).mCustomScrollbar("update");
          }
        });
      });

    }
  };

  // After window resize and deplay time complete, do this
  $(window).bind('resizeEnd', function() {
    homepageMarqueeSlideshow(true);
  });

  // Hide all images in the slideshow homepage marquee except the first
  var homepageMarqueeSlideshow = function(onWinResize) {
      $('div[class^=banner--hero] img[class^="adaptive-image"]').slice(1).hide();

      // Adds height style to marquee area to prevent page jump issue on mobile
      var hp_marquee_height = $('div[class^=banner--hero]:first img[class^="adaptive-image"]').height();
      if (hp_marquee_height > 0) {
        $('.homepage_marquee').css('height', hp_marquee_height);
      }
  }

  // On window resize, delay 300ms before continuing
  $(window).resize(function() {
    if(this.resizeTO) clearTimeout(this.resizeTO);
    this.resizeTO = setTimeout(function() {
      $(this).trigger('resizeEnd');
    }, 300);
  });

  // Current Issues
  Drupal.behaviors.currentIssues = {
    attach: function(context, settings) {
      Response.action(function() {
        if (R.band(0, Breakpoints.LARGE)) {
          $('.pane-aclu-topics-current-issues .taxonomy-term', context).each(function() {
            $(this).find('.field-image').prependTo($(this).find('.content'));
          });
        }
      });
      Response.action(function() {
        if (R.band(Breakpoints.LARGE)) {
          $('.pane-aclu-topics-current-issues .taxonomy-term', context).each(function() {
            $(this).find('.field-image').prependTo(this);
          });
          // Add inner wrappers so we can set tricky anchor positions.
          $('.pane-aclu-topics-current-issues', context).once().wrapInner('<div class="inner" />');
        }
      });
    }
  }

  // KYR Search / Filters
  Drupal.behaviors.kyrSearchFilters = {
    attach: function(context, settings) {
      Response.action(function() {
        if (R.band(0, Breakpoints.MEDIUM)) {
          $('.pane-listings--title', context).text('Search and Filter');
          $('.pane-listings').accordion({
            heightStyle: "content",
            collapsible: true,
            active: false,
            animate: "easeOutExpo",
            header: "h3.pane-listings--title",
            icons: "header"
          });
        }
      });
      Response.action(function() {
        if (R.band(Breakpoints.MEDIUM)) {
          $('.pane-listings--title', context).text('Filters');
          $('.pane-listings.ui-accordion', context).accordion("destroy");
          $('.pane-listings .search-filters', context).show();
        }
      });
    }
  }

  // Follow the Case
  Drupal.behaviors.followCase = {
    attach: function(context, settings) {
      Response.action(function() {

        var follow_tabs = $('.pane-aclu-case-follow-case .follow-case > .item-list');

        // Everything from 0 to 767
        if (R.band(0, 767)) {
          // Destroy the tabs from medium+ breakpoint.
          $('.follow-case.ui-tabs', context).tabs("destroy");

          // Modify the markup a bit to allow for an accordion.
          $(follow_tabs).find('li a', context).each(function() {
            var pane_name =$(this).attr("href").substring(1);
            $(this).parent().attr('id', pane_name + '-parent');
            $(this).insertBefore($('.tab-pane#' + pane_name)).wrap('<h3 class="accordion-header"></h3>');
          });

          // Convert to accordion.
          $('.follow-case', context).accordion({
            heightStyle: "content",
            collapsible: true,
            active: false,
            animate: "easeOutExpo",
            header: "h3.accordion-header",
            icons: "header"
          });

        }

        // Everything 768 and up
        if (R.band(Breakpoints.MEDIUM)) {

          // Unset and destroy the accordion from small breakpoint.
          $('.follow-case.ui-accordion', context).accordion("destroy");
          $('.follow-case > h3', context).find('a').unwrap().each(function() {
            var pane_name =$(this).attr("href").substring(1);
            $(this).prependTo($('.follow-case .item-list ul li#' + pane_name + '-parent'));
          });

          // Convert to tabs.
          $('.follow-case', context).tabs();

        }
      });
    }
  }

  // Sidebar Left Shuffle
  Drupal.behaviors.sidebarLeftShuffle = {
    attach: function (context, settings) {

      var sidebarLeft = $('.panel-left-1.sidebar-left', context).not('.section-search .panel-left-1.sidebar-left, .node-type-foia-collection .panel-left-1.sidebar-left');
      var contentColumn = $('.panel-main-1.content-column', context).not('.section-search .panel-main-1.content-column, .node-type-foia-collection .panel-main-1.content-column');

      // Set a class on sidebar left's original parent, so we can return things to their original position.
      $(sidebarLeft).parent().addClass('sidebar-left-original-parent');

      // Small/Medium layouts
      Response.action(function() {
        // Move under page title.
        if (Response.band(0, 960)) {
          sidebarLeft.insertAfter(contentColumn);
        }
      });
      // Large layouts
      Response.action(function() {
        if (Response.band(961)) {
          // Move back to original location.
          contentColumn.next('.sidebar-left').prependTo('.sidebar-left-original-parent');
        }
      });

    }
  };

  // Sidebar Right Shuffle
  Drupal.behaviors.sidebarRightShuffle = {
    attach: function (context, settings) {

      var sidebarRight = $('.section-blog.page-taxonomy-term .panel-right-2.sidebar-right', context);
      var contentColumn = $('.section-blog.page-taxonomy-term .panel-main-2.content-column', context);

      // Set a class on sidebar left's original parent, so we can return things to their original position.
      $(sidebarRight).parent().addClass('sidebar-right-original-parent');

      // Small/Medium layouts
      Response.action(function() {
        // Move under page title.
        if (Response.band(0, Breakpoints.LARGE)) {
          sidebarRight.insertBefore(contentColumn);
        }
      });
      // Large layouts
      Response.action(function() {
        if (Response.band(Breakpoints.LARGE)) {
          // Move back to original location.
          contentColumn.next('.sidebar-right').prependTo('.sidebar-right-original-parent');
        }
      });

    }
  };

  // Infographic Shuffle
  Drupal.behaviors.infographicShuffle = {
    attach: function (context, settings) {

      var sidebarRight = $('.node-type-infographic .panel-right-2.sidebar-right', context);
      var infographic = $('.pane-node-field-infographic-image-file', context);

      // Set a class on share icons' original parent, so we can return things to their original position.
      $(sidebarRight).parent().addClass('sidebar-right-original-parent');

      // Medium layouts
      Response.action(function() {
        // Move under page title.
        if (Response.band(0, 960)) {
          sidebarRight.insertAfter(infographic);
        }
      });
      // Large layouts
      Response.action(function() {
        if (Response.band(961)) {
          // Move back to original location.
          infographic.next('.sidebar-right').prependTo('.sidebar-right-original-parent');
        }
      });

    }
  };

  // Asset Shuffle
  Drupal.behaviors.assetShuffle = {
    attach: function (context, settings) {

      var sidebarRight = $('.node-type-asset .panel-right-2.sidebar-right', context);
      var asset = $('.pane-node-field-document-cloud-document', context);

      // Set a class on share icons' original parent, so we can return things to their original position.
      $(sidebarRight).parent().addClass('sidebar-right-original-parent');

      // Medium layouts
      Response.action(function() {
        // Move under page title.
        if (Response.band(0, 960)) {
          sidebarRight.insertAfter(asset);
        }
      });
      // Large layouts
      Response.action(function() {
        if (Response.band(961)) {
          // Move back to original location.
          asset.next('.sidebar-right').prependTo('.sidebar-right-original-parent');
        }
      });

    }
  };


  // Header Search responsive actions
  Drupal.behaviors.headerSearchResponsive = {
    attach: function(context, settings) {
      $('body', context).once('headerSearchResponsive', function () {

        // When crossing from one "width" breakpoint to another...
        Response.crossover(function() {

          // Hide Search box when crossing into small/medium
          if (R.band(Breakpoints.SMALL, Breakpoints.LARGE)) {
            $('#header #block-search-api-page-search-aclu .content', context)
              .hide()
              .addClass('closed')
              .removeClass('open')
              .prev('.search-header').addClass('closed').removeClass('open');
          }

        }, "width");
      });
    }
  };

  // Header Get Updates responsive actions
  Drupal.behaviors.getUpdatesResponsive = {
    attach: function(context, settings) {
      $('body', context).once('getUpdatesResponsive', function () {

        // When crossing from one "width" breakpoint to another...
        Response.crossover(function() {

          // Hide Get Updates box if open when crossing into Small
          if (R.band(Breakpoints.SMALL, Breakpoints.MEDIUM)) {
            $('#block-aclu-social-get-updates', context).hide().removeClass('open');
            $('.menu-item-get-updates', context).removeClass('open');
          }

          // Hide Get Updates box if open when crossing into Large. This doesn't
          // actually close it since a CSS !important keeps it open for Large,
          // but it will keep it closed if you go back down to Medium again.
          if (R.band(Breakpoints.LARGE)) {
            $('#block-aclu-social-get-updates', context).hide().removeClass('open');
            $('.menu-item-get-updates', context).removeClass('open');
          }

        }, "width");
      });
    }
  };

  // iframe resizer for content that is not responive add a scroller
  // Hack: can be removed if they fix their content
  Drupal.behaviors.iframeResizerNonResponsiveContent = {
    attach: function (context, settings) {
      Response.action(function() {
        if (R.band(Breakpoints.MEDIUM)) {
          // If content is responsive no need for a scroller
          // so use the id to target the league of women voters iframe
          $("#lwv_srch_embed").attr("scrolling", "yes");
        }
      });
    }
  };

})(jQuery, Drupal, this, this.document, Response);


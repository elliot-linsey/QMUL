  // Format the date provided on page from August 11, 2020 to yyyy--mm-dd
  $.alterDateFormat = function(dateVal) {
    var d = new Date(dateVal);
    var day = d.getDate();
    var month = d.getMonth() + 1;
    var year = d.getFullYear();
    if (day < 10) {
        day = '0' + day;
    }
    if (month < 10) {
        month = '0' + month;
    }
    var date = year + '-' + month + '-' + day;
    return date;
  };

  // Make sure this runs once, sending article information to Heap
  function sendDataToHeap() {
    if (!isLoaded) {
      isLoaded = true;
      if (typeof heap !== 'undefined' && typeof heap.track !== 'undefined') {
        var hpArticleTitle = $('.hp__article_title').text();
        var hpArticleAuthor = [];
        var hpArticleKeywords = [];
        var hpArticleDateModified;

        // Grab data from ld+json for blog posts
        if ($('#ld-json-schema').length) {
          var json_data = JSON.parse(document.querySelector('#ld-json-schema').innerText)
          var json_author = json_data['author'];
          $.each(json_author, function(i, auth) {
            hpArticleAuthor.push(auth['name']);
          });
          var hpArticleDatePublished = json_data['datePublished'].replace('T', ' ');
          hpArticleDateModified = json_data['dateModified'];
          hpArticleKeywords = json_data['keywords'];
        }
        // if ld+json data doesn't exist such as on cases and press releases
        else {
          $('#hp__article_keyword li a').once().each(function() {
            hpArticleKeywords.push($(this).text());
          });
          if ($('.hp__article_date_modified').length) {
            hpArticleDateModified = $.alterDateFormat($('.hp__article_date_modified .date-display-single').text());
          }
        }

        // Send specific data to Heap
        heap.track('Article Metadata', {
          articleTitle: hpArticleTitle ? hpArticleTitle : '',
          articleDatePublished: hpArticleDatePublished ? hpArticleDatePublished : '',
          articleAuthors: hpArticleAuthor ? hpArticleAuthor.toString() : '',
          articleKeywords: hpArticleKeywords ? hpArticleKeywords.toString() : '',
          articleDateModified: hpArticleDateModified ? hpArticleDateModified : ''
        });
      }
    }
  }

  // Send data to heap
  Drupal.behaviors.sendToHeap = {
    attach: function(context, settings) {
      $(window).on('load', function() {
        sendDataToHeap();
      });
    }
  }


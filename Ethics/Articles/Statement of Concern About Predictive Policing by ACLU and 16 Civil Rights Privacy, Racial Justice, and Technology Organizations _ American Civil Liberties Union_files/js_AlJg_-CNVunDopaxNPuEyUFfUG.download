            currentAmount = FrTotal.donationSemiAnnualFreqAmounts.filter(':checked').val();
          }

          if (recursSelected === 'sb_fs_annually') {
            currentAmount = FrTotal.donationAnnuallyFreqAmounts.filter(':checked').val();
          }
